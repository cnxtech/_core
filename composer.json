{
    "name": "codex/core",
    "description": "The core package for codex. This is the minimum required package to get codex up and running.",
    "keywords": ["framework", "laravel", "documentation", "docs", "markdown", "codex"],
    "homepage": "http://codex-project.ninja",
    "authors": [{"name": "Robin Radic", "email": "robin@radic.nl"}],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "laradic/support": "1.0.*",
        "illuminate/contracts": "~5.0",
        "herrera-io/version": "~1.1",
        "erusev/parsedown": "~1.5",
        "erusev/parsedown-extra": "~0.7",
        "symfony/yaml": "~2.7|~3.0",
        "vierbergenlars/php-semver": "3.0.*",
        "nicmart/tree": "0.2.*",
        "radic/blade-extensions": "~6.2",
        "myclabs/php-enum": "~1.4",
        "symfony/config": "~3.0",
        "doctrine/common": "~2.6",
        "doctrine/annotations": "~1.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.2"
    },
    "autoload": {
        "psr-4": {
            "Codex\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codex\\Tests\\Core\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
